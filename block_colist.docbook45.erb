<%#encoding:UTF-8%><calloutlist<%= @id && %( id="#@id") %>><%= title? ? %(<title>#{title}</title>) : nil %><%content.each_with_index do |li, index| %><callout arearefs="<%= "#{li.attr('coids')}#{if @attributes["callout_aliases"] then " " + @attributes["callout_aliases"].split(" ")[index] end}"%>"><para><%= li.text %></para><% if li.blocks? %><%= li.content %><% end %></callout><% end %> </calloutlist>