<%#encoding:UTF-8%><% if attr? :style, 'bibliography' %>
<bibliodiv#{common_attrs_erb}>#{title_tag}
  <% content.each do |li| %>
    <bibliomixed>
      <bibliomisc><%= li.text %></bibliomisc>
      <% if li.blocks? %>
<%= li.content %>
      <% end %>
    </bibliomixed>
  <% end %>
</bibliodiv>
<% else %>
<itemizedlist>
  <% content.each do |li| %>
    <listitem>
      <para><%= li.text %></para>
      <% if li.blocks? %>
<%= li.content %>
      <% end %>
    </listitem>
  <% end %>
</itemizedlist>
<% end %>